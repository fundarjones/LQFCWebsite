<script>
   $('#password, #confirm_password').on('keyup', function () {
       let password = document.getElementById("password").value;
       let cnfrmPassword = document.getElementById("confirm_password").value;
       let messagepw = document.getElementById("message-pw");
       console.log(" Password:", password,'\n',"Confirm Password:",cnfrmPassword);
   
       if(password.length != 0){
           if(password == cnfrmPassword){
            messagepw.textContent = "Passwords match!";
            messagepw.style.color = "#1dcd59";
           }
           else{
            messagepw.textContent = "Passwords do not match!";
            messagepw.style.color = "#ff4d4d";
           }
       }
       else{
         messagepw.textContent = "";
       }
   });

   $('#name').on('input', function() {
  var c = this.selectionStart,
      r = /[^a-z a-z]/gi,
      v = $(this).val();
  if(r.test(v)) {
    $(this).val(v.replace(r, ''));
    c--;
  }
  this.setSelectionRange(c, c);
});

   </script>
   
