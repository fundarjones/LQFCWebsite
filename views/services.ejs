  <link rel="stylesheet" href="../assets/css/libraries.css">
  <link rel="stylesheet" href="../assets/css/style.css">
</head>

<body>
  <div class="wrapper">
    <div class="preloader">
      <div class="loading"><span></span><span></span><span></span><span></span></div>
    </div><!-- /.preloader -->

   <!-- =========================
        Header
    =========================== -->
    <header class="header header-layout1">
      
      <nav class="navbar navbar-expand-lg is-sticky">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">
            <img src="assets/images/logo/logo.png" style="width:100%; height:auto; max-width:130px;" class="logo-dark" alt="logo">
          </a>
          <button class="navbar-toggler" type="button">
            <span class="menu-lines"><span></span></span>
          </button>
          <div class="collapse navbar-collapse" id="mainNavigation">
            <ul class="navbar-nav ml-auto">
              <li class="nav__item">
                <a href="/" class="nav__item-link">Home</a>
              </li><!-- /.nav-item -->
              <li class="nav__item">
                <a href="/about" class="nav__item-link">About</a>
              </li><!-- /.nav-item -->
              <li class="nav__item">
                <a href="/contact" class="nav__item-link">Contact</a>
              </li><!-- /.nav-item -->
              <li class="nav__item">
                <a href="/services" class="nav__item-link active">Services</a>
              </li><!-- /.nav-item -->
              <li class="nav__item">
                <a href="/lqfc-branches" class="nav__item-link">Branches</a>
              </li><!-- /.nav-item -->
              <li class="nav__item">
                <a href="/lqfc-doctors" class="nav__item-link">Doctors</a>
              </li><!-- /.nav-item -->
              
            </ul><!-- /.navbar-nav -->
            <button class="close-mobile-menu d-block d-lg-none"><i class="fas fa-times"></i></button>
          <!-- /.navbar-collapse -->
          <div class="d-none d-xl-flex align-items-center position-relative ml-30"> 
            <a href="" class="btn btn__primary btn__rounded ml-30" data-toggle="modal" data-target="#exampleModalCenter">
              <span>LOGIN</span>
            </a>
          </div>
        </div>
        </div>
        </div><!-- /.container -->
      </nav><!-- /.navabr -->
    </header><!-- /.Header -->

    <!-- ============================
        Modal
    ============================== -->

    <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <div class="col-md-12 text-center">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            <h5 class="modal-title" id="exampleModalLongTitle">LOGIN</h5>
            
           </div>
          </div>
          <div class="modal-body text-center">
            Welcome to Lagman Qualicare Family Clinic!</div>
          <div class="modal-footer ">
            <div class="col-md-12 text-center">
              <button type="button" onclick="document.location='/patient-login'" class="btn btn__primary btn__outlined btn__rounded">I'm a Patient</button>
              <button type="button" onclick="document.location='/doctor-login'" class="btn btn__primary btn__outlined btn__rounded">I'm a Doctor</button>
              <button type="button" onclick="document.location='/staff-login'" class="btn btn__primary btn__outlined btn__rounded">I'm a Staff</button>
            </div>
          </div>
        </div>
      </div>
    </div>

      <!-- ============================
        Modal 2
    ============================== -->

    <div class="modal fade" id="make-appointment" tabindex="-1" role="dialog">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <div class="col-md-12 text-center">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
              <img src="../assets/images/logo/logo.png" width="150px" class="logo-light" alt="logo">
           </div>
          </div>
          <div class="modal-body text-center">
            When booking an appointment, you need to be logged in first in order for us to get your details.</div>
          <div class="modal-footer ">
            <div class="col-md-12 text-center">
              <button type="button" onclick="document.location='local/patient/login.html'" class="btn btn__primary btn__outlined btn__rounded">Sign In</button>
              <button type="button" onclick="document.location='local/patient/signup.html'" class="btn btn__primary btn__outlined btn__rounded">Sign Up</button>
              </div>
          </div>
        </div>
      </div>
    </div>   
 


    <!-- ========================
        Services Layout 1
    =========================== -->
    <section class="services-layout1 mt-90 pt-130">
      <div class="container">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-6 offset-lg-3">
            <div class="heading text-center mb-60">
              <h2 class="heading__subtitle">The Best Medical & General Practice Care</h2>
              <h3 class="heading__title">Different Aspects of Medical Practice for Your Whole Family</h3>
            </div><!-- /.heading -->
          </div><!-- /.col-lg-6 -->
        </div><!-- /.row -->
        <div class="row">
          <!-- service item #1 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">Hypertension & Cardiovascular Diseases</h4>
                <p class="service__desc">We provide the environment for
                  specialized procedures in the diagnosis of
                  patients with known or suspected heart-related symptoms and disorders.
                </p>
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
          <!-- service item #2 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">General Internal Medicince</h4>
                <p class="service__desc">Experts in health
                  promotion, disease prevention, and the care of problems both simple and complex, acute and chronic.
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
          <!-- service item #3 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">Complete Adult Pediatric Immunization</h4>
                <p class="service__desc">Immunizations are critical in protecting not only children but also
                  adults against damaging and potentially life-threatening diseases.
                </p>
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
          <!-- service item #4 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">EENT</h4>
                <p class="service__desc">Specialized unit for patients with focus on ear, nose and
                  throat related concerns. The clinic has a seamless
                  transition from outpatient services to referrals for more specialized procedures.
                </p>
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
          <!-- service item #5 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">Anti Rabies Vaccine</h4>
                <p class="service__desc">We treat animals as a member of our family and just like humans,
                  we want to give them the best care possible. And we will promote this through vaccination
                  and proper information dissemination.
                </p>
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
          <!-- service item #6 -->
          <div class="col-sm-12 col-md-6 col-lg-4">
            <div class="service-item">
              <div class="service__content">
                <h4 class="service__title">2021 Quadrivalent Flu Vaccine</h4>
                <p class="service__desc">Stay up-to-date on getting recommended vaccines.
                  Immunization schedules for people of all ages and health care professionals to use.
                </p>
              </div><!-- /.service__content -->
            </div><!-- /.service-item -->
          </div><!-- /.col-lg-4 -->
        </div><!-- /.row -->
      </div><!-- /.container -->
    </section><!-- /.Services Layout 1 -->

 <!-- ========================
      Footer
    ========================== -->
    <footer class="footer">
      <div class="footer-primary">
        <div class="container">
          <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-3">
              <div class="footer-widget-about">
                <img src="../assets/images/logo/logo-light.png"  alt="logo" class="mb-30">
                <p class="color-gray">Our goal is to deliver quality of care in a courteous, respectful, and
                  compassionate manner. We hope you will allow us to care for you and strive to be the first and best
                  choice for your family healthcare.
                </p>
              </div><!-- /.footer-widget__content -->
            </div><!-- /.col-xl-2 -->
            <div class="col-sm-6 col-md-6 col-lg-2 offset-lg-1">
              <div class="footer-widget-nav">
                  <ul class=" list-unstyled list-items-light">
                    <li><a href="/about">About Us</a></li>
                    <li><a href="/contact">Contact Us</a></li>
                    <li><a href="/lqfc-doctors">Our Doctors</a></li>
                    <li><a href="/services">Our Services</a></li>
                    <li><a href="/branches">Our Branches</a></li>
                </ul>
              </div><!-- /.footer-widget__content -->
            </div><!-- /.col-lg-2 -->
            <div class="col-sm-12 col-md-6 col-lg-6">
              <div class="footer-widget-contact">
                <h4 class="contact-panel__title color-heading">Contacts</h4>
                <p class="contact-panel__desc mb-30">Feel free to contact us
                  with any general or medical enquiry.
                </p>
                <ul class="contact-list list-horizontal list-unstyled">
                  <% branch.forEach(function(branches){ %>
                  <li>
                    <%= branches.branch_name %><br>
                    <a href="tel:+<%= branches.phone %>">
                      <i class="icon-phone"></i> <span><%= branches.phone %></span>
                    </a>
                  </li>
                  <% }) %>
                </ul>
                

                <div class="d-flex align-items-center">
                  <ul class="social-icons list-unstyled mb-0">
                    <li><a href="https://www.facebook.com/Lagman-Qualicare-Family-Clinic-Laboratory-and-Pharmacy-104828698310093"><i class="fab fa-facebook-f"></i></a></li>
                  </ul><!-- /.social-icons -->
                </div>
              </div><!-- /.footer-widget__content -->
            </div><!-- /.col-lg-2 -->
          </div><!-- /.row -->
        </div><!-- /.container -->
      </div><!-- /.footer-primary -->
      <div class="footer-secondary">
        <div class="container">
          <div class="row align-items-center">
            <div class="col-sm-12 col-md-6 col-lg-6">
              <span class="fz-14">&copy; </span>
              <a class="fz-14 color-primary" href="/">2022 Lagman Qualicare Family Clinic</a>
            </div><!-- /.col-lg-6 -->
            <div class="col-sm-12 col-md-6 col-lg-6">
              <nav>
                <ul class="list-unstyled footer__copyright-links d-flex flex-wrap justify-content-end mb-0">
                  <li><a href="/terms-conditions">Terms & Conditions</a></li>
                </ul>
              </nav>
            </div><!-- /.col-lg-6 -->
          </div><!-- /.row -->
        </div><!-- /.container -->
      </div><!-- /.footer-secondary -->
    </footer><!-- /.Footer -->
    <button id="scrollTopBtn"><i class="fas fa-long-arrow-alt-up"></i></button>
  </div><!-- /.wrapper -->

  <script src="../assets/js/jquery-3.5.1.min.js"></script>
  <script src="../assets/js/plugins.js"></script>
  <script src="../assets/js/main.js"></script>
</body>

</html>